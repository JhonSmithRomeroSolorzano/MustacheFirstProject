<html>
  <head>
    <link rel="stylesheet" href="/css/bootstrap.css" type="text/css">
    <link rel="stylesheet" href="/css/style.css" type="text/css">
  </head>
  <body class="container">
    <span> {{timestamp}}</span>  
    <h1>Name: {{name}}</h1>
    <img class="profile-image" src= {{image}} />
    <h2>Company: {{company}} </h2>

    <h3>Languages: </h3>
    {{languages}}
    <ol>
      {{#languages}}
      <li>
        {{.}}
      </li>
      {{/languages}}
    </ol>

    <form action="/addprofile" method="POST">
      <input type="name" name="name" placeholder="Name"/> <br/>
      <input type="username" name="username" placeholder="Username"/> <br/>
      <input type="company" name="company" placeholder="Company"/> <br/>
      <input type="languages" name="languages" placeholder="Language"/> <br/>
      <input type="submit" value="Add Profile"/>
    </form>
  </body>
</html>